<%- include('../Dashboardhead.ejs') %>
            <div class="col-md-9">
              <div class="row">
                <h1>About Us Data</h1>
              </div>
              <!--table-->
                <table class="table table-bordered table-striped">
                    <thead class="bg-success">
                      <tr>
                        <th scope="col">Id No.</th>
                        <th scope="col">Content</th>
                        <th scope="col">Image</th>
                        <th scope="col">Action</th>
                      </tr>                   
                    </thead>
                    <tbody>
                      <% d.forEach((item)=>{ %>
                        <tr>
                          <th scope="row"><%= item._id %></th>
                          <td><%= item.content.slice(0,100) %></td>
                          <td><img src="<%= item.image.url %>" class="displayimg"></td>
                          <td>
                              <a href="/aboutusedit/<%= item._id %>"><button class="btn btn-success fa fa-edit"></button></a>
                          </td>
                        </tr>                          
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>



  <!--footer-->
<div style="padding-top: 5cm;"></div>
<%- include('../../footer.ejs') %>