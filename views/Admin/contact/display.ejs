<%- include('../Dashboardhead.ejs') %>
            <div class="col-md-9">
              <div class="row">
                <h1>Queries</h1>
              </div>
              <!--table-->
                <table class="table table-bordered table-striped">
                    <thead class="bg-success">
                      <tr>
                        <th scope="col">Id No.</th>
                        <th scope="col">First Name</th>
                        <th scope="col">Last Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">Phone Nmber</th>
                        <th scope="col">Query</th>
                        <th scope="col">Action</th>
                      </tr>                   
                    </thead>
                    <tbody>
                      <% d.forEach((item)=>{ %>
                        <tr>
                          <th scope="row"><%= item._id %></th>
                          <td><%= item.fname %></td>
                          <td><%= item.lname %></td>
                          <td><%= item.email %></td>
                          <td><%= item.number %></td>
                          <td><%= item.query.slice(0,100) %></td>
                          <td>
                            <a href="/contactdelete/<%= item._id %>"><button class="btn btn-success fa fa-trash-can"></button></a>
                            <a href="/contactview/<%= item._id %>"><button class="btn btn-success fa fa-eye"></button></a>
                          </td>
                        </tr>                          
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>



  <!--footer-->
<div style="padding-top: 5cm;"></div>
<%- include('../../footer.ejs') %>